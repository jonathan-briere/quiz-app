{"remainingRequest":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\src\\views\\Game.vue?vue&type=style&index=0&id=1b588954&scoped=true&lang=css&","dependencies":[{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\src\\views\\Game.vue","mtime":1577945937427},{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Syntist\\Documents\\Projects\\quiz-app\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Game.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4LA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"Game.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div class=\"game\">\n    <div class=\"heading\">\n      <div>\n        <img class=\"responsive\" src=\"../assets/logo.jpg\" width=\"100\" height=\"100\" />\n      </div>\n      <div>\n        <h2 style=\"padding-top: 13px;\">GK Quiz</h2>\n      </div>\n      <div>\n        <img class=\"responsive\" src=\"../assets/sharp.jpg\" width=\"200\" height=\"100\" />\n      </div>\n    </div>\n    <div class=\"normal\">\n      <div class=\"question\">\n        <div>\n          <p style=\"font-weight: bold;\">Q{{qno+1}}) {{show.q}}</p>\n          <ul class=\"listQ\" style=\"list-style-type:none\">\n            <li class=\"innerL\" id=\"reset\" v-for=\"(option,index) in show.op\" :key=\"index\">\n              <input type=\"radio\" v-bind:value=\"index + '.' + option\" v-model=\"picked\" />\n              <label v-bind:for=\"index\" v-bind:id=\"index\">{{option}}</label>\n            </li>\n          </ul>\n        </div>\n\n        <div>\n          <a href=\"#/game\" @click=\"clickHint()\">\n            <img class=\"responsive\" src=\"../assets/hint.jpg\" title=\"Hint\" width=\"25\" height=\"25\" />\n          </a>\n          <a href>\n            <img\n              style=\"padding-left:100px\"\n              class=\"responsive\"\n              src=\"../assets/close.jpg\"\n              title=\"Exit\"\n              width=\"25\"\n              height=\"25\"\n            />\n          </a>\n          <br />\n          <img class=\"reponsive\" src=\"../assets/earth.jpeg\" width=\"150\" height=\"150\" />\n        </div>\n      </div>\n\n      <div class=\"bottom\">\n        <div class=\"listQ\">IQ Level={{point}}</div>\n        <div>\n          <a class=\"next\" href=\"#/game\" @click=\"checkAns()\">\n            <img src=\"../assets/next.png\" width=\"25\" height=\"25\" />\n          </a>\n        </div>\n        <div>\n          {{time}}\n        </div>\n      </div>\n    </div>\n\n    <app-timer @sendTime=\"updateTime\" @stopTimer=\"getstopTimer\"/>\n  </div>\n</template>\n\n\n<script>\nimport Timer from \"../components/timer.vue\";\nexport default {\n  components: {\n    \"app-timer\": Timer\n  },\n  data() {\n    return {\n      picked: null, //Ans that is picked.. using selection\n      point: 0, // Points Show the User Score based on the given answers.\n      time: null, //Timer that is being accessed from time.vue component\n      stop: null, //Stop function to stop the timer, that is bound to stopTimer in vue\n      question: [\n        {\n          q: \"What is the capital of Pakistan\",\n          op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n          hint: \"Start with I\",\n          ans: \"Islamabad\"\n        },\n\n        {\n          q: \"What is the capital of Punjab\",\n          op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n          hint: \"Start with L\",\n          ans: \"Lahore\"\n        },\n\n        {\n          q: \"What is the capital of Sindh\",\n          op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n          hint: \"Start with K\",\n          ans: \"Karachi\"\n        }\n      ],\n\n      qno: 0, //Indexing Of Question No\n      show: \"\"\n    };\n  },\n  methods: {\n    checkAns: function() { //Check the Ans\n      this.picked = this.picked.split(\".\");\n      if (this.picked[1] == this.show.ans) {\n        this.correctAns();\n      } else {\n        this.wrongAns();\n      }\n    },\n    //Next Function Basically, Brings up the next question.\n    next: function() {\n      if (this.qno < this.question.length - 1) {\n        this.qno++;\n        this.show = this.question[this.qno];\n      } else {\n        this.stop();\n        this.result();\n      }\n    },\n\n    correctAns: function() {  // This Function is run if the ans is Correct.\n      var obj = document.getElementById(this.picked[0]);\n      obj.style.backgroundColor = \"#63E25D\";\n      this.$fire({\n        title: \"Correct\",\n        text: \"Answer\",\n        type: \"success\",\n        timer: 3000\n      }).then(() => {\n        obj.style.backgroundColor = \"#c4c4c4\";\n        this.point += 2;\n        this.next(); //When the Answer is connect. point+2 and next question.\n      });\n    },\n    wrongAns: function() { // This Function is run if the ans is Wrong.\n      var obj = document.getElementById(this.picked[0]);\n      obj.style.backgroundColor = \"#C70039\";\n      var i = 0;\n      for (i = 0; i < 4; i++) {\n        if (this.show.op[i] == this.show.ans) break;\n      }\n      var obj2 = document.getElementById(i);\n      obj2.style.backgroundColor = \"#63E25D\";\n      this.$fire({\n        title: \"Wrong\",\n        text: \"Correct Answer is \" + this.show.ans,\n        type: \"error\",\n        timer: 3000\n      }).then(() => {\n        obj.style.backgroundColor = \"#c4c4c4\";\n        obj2.style.backgroundColor = \"#c4c4c4\";\n        this.point -= 2;\n        this.next(); //When the Answer is connect. point+2 and next question.\n      });\n    },\n\n    clickHint: function() { //Hint Function to Show Hint for Questions\n      this.$alert(this.show.hint, \"\", \"info\");\n    },\n\n    result: function() {  //Result Function Shows the result\n      this.$alert(\"You Scored \" + this.point + \" in \" + this.time, \"Result\").then(() => {\n        this.$router.push(\"/\");\n      });\n    },\n\n    //events (Child To Parents)\n    updateTime: function(updatedTime) {\n      this.time = updatedTime;\n    },\n\n    getstopTimer: function(stopFn) {\n      this.stop = stopFn;\n    },\n    //\n\n  },\n\n  //lifecycle\n  created() {\n    this.show = this.question[this.qno];\n  }\n};\n</script>\n\n\n<style scoped>\n.game {\n  width: 100%;\n  background-color: #e5e5e5;\n  padding-bottom: 50px;\n}\n\n.game .heading {\n  padding: 10px 9px 5px 5px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  background-color: #c4c4c4;\n  text-align: center;\n  margin-bottom: 80px;\n}\n\n.question {\n  padding: 4px 9px 4px 9px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  text-align: left;\n}\n\n.normal {\n  background-color: #c4c4c4;\n}\n\n.bottom {\n  padding-top: 10px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  text-align: left;\n  width: 80%;\n  margin: auto;\n}\n\n.innerL {\n  padding-bottom: 5px;\n}\n\n</style>"]}]}