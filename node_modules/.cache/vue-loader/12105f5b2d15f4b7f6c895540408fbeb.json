mal, correct:correct, wrong:wrong}\">\n            <div>\n                <p style=\"font-weight: bold;\">Q) {{show.q}}</p>\n                <ul style=\"list-style-type:none\">\n                    <li v-for=\"(option,index) in show.op\" :key=\"index\">\n                        <input type=\"radio\" v-bind:id=\"index\" v-bind:value=\"option\" v-model=\"picked\">\n                        <label v-bind:for=\"index\">{{option}}</label>\n                    </li>\n                </ul>\n            </div>\n            <div>\n                <a href=\"\"><img class=\"responsive\" src=\"../assets/close.jpg\" width=\"25\" height=\"25\"></a><br>\n                <img class=\"reponsive\" src=\"../assets/earth.jpeg\" width=\"200\" height=\"200\">\n            </div>\n            <div>\n                <p>IQ Level={{point}}</p>\n            </div>\n            <div>\n                <a @click=\"checkAns()\"><img src=\"../assets/next.jpg\" width=\"25\" height=\"25\" ></a>\n                <p>Clock</p>\n            </div>\n        </div>\n    \n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            normal: true,\n            correct: false,\n            wrong: false,\n            picked: '', //The Answer is Stored in Picked Variable\n            point: 0, // Points Show the User awdawdScorte based on the give answers.\n            question: [{\n                    q: 'What is the capital of Pakistan',\n                    op: ['Lahore', 'Karachi', 'Pashawar', 'Islamabad'],\n                    ans: 'Islamabad'\n                },\n\n                {\n                    q: 'What is the capital of Punjab',\n                    op: ['Lahore', 'Karachi', 'Pashawar', 'Islamabad'],\n                    ans: 'Lahore'\n                },\n\n                {\n                    q: 'What is the capital of Sindh',\n                    op: ['Lahore', 'Karachi', 'Pashawar', 'Islamabad'],\n                    ans: 'Karachi'\n                },\n            ],\n            qno: 0, //Indexing Of Question No\n            show: '',\n        }\n    },\n    methods: {\n        checkAns: function() {\n            if (this.picked == this.show.ans) {\n                alert(\"correct\");\n                this.point += 2;\n                this.correctAns();\n                //          this.next(); //When the Answer is connect. point+2 and next question.\n            } else {\n                alert(\"wrong\");\n                this.point -= 2;\n                this.wrongAns();\n                //         this.next(); //When the Answer is connect. point-2 and next question.\n            }\n        },\n        //Next Function Basically, Brings up the next question. \n        next: function() {\n            if (this.qno < this.question.length - 1) {\n                this.qno++;\n                this.show = this.question[this.qno];\n                this.correct = false;\n                this.wrong = false;\n                this.normal = true;\n            } else {\n                console.log(this.qno)\n            }\n        },\n\n        correctAns: function() {\n            this.normal = false;\n            this.correct = true;\n            this.wrong = false;\n        },\n        wrongAns: function() {\n            this.normal = false;\n            this.correct = false;\n            this.wrong = true;\n        }\n\n    },\n\n    //lifecycle\n    created() {\n        this.show = this.question[this.qno];\n    }\n};\n</script>\n\n\n<style scoped>\n.game {\n    width: 100%;\n    background-color: #E5E5E5;\n}\n\n.game .heading {\n    padding: 10px 9px 5px 5px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    background-color: #C4C4C4;\n    text-align: center;\n    margin-bottom: 80px;\n}\n\n.question {\n    padding: 4px 9px 4px 9px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    text-align: left;\n}\n\n.normal {\n    background-color: #C4C4C4;\n}\n\n.correct {\n    background-color: #63E25D;\n}\n\n.wrong {\n    background-color: #C70039;\n}\n</style>"]}]}