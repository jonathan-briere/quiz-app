stion:true, normal:normal, correct:correct, wrong:wrong}\">\n            <div>\n                <p style=\"font-weight: bold;\">Q) {{show.q}}</p>\n                <ul style=\"list-style-type:none\">\n                    <li v-for=\"(option,index) in show.op\" :key=\"index\">\n                        <input type=\"radio\"  v-bind:value=\"index + '.' + option\" v-model=\"picked\" />\n                        <label v-bind:for=\"index\" v-bind:id=\"index\" >{{option}}</label>\n                    </li>\n                </ul>\n            </div>\n            <div>\n                <a href>\n              <img class=\"responsive\" src=\"../assets/close.jpg\" width=\"25\" height=\"25\" />\n            </a>\n                <br />\n                <img class=\"reponsive\" src=\"../assets/earth.jpeg\" width=\"200\" height=\"200\" />\n            </div>\n            <div>\n                <p>IQ Level={{point}}</p>\n            </div>\n            <div>\n                <a @click=\"checkAns()\">\n              <img src=\"../assets/next.jpg\" width=\"25\" height=\"25\" />\n            </a>\n                <p>Clock</p>\n            </div>\n        </div>\n    </div>\n</template>\n\n\n<script>\nexport default {\n    data() {\n        return {\n            normal: true,\n            correct: false,\n            wrong: false,\n            picked: null,\n            id: null,\n            point: 0, // Points Show the User awdawdScorte based on the give answers.\n            question: [{\n                    q: \"What is the capital of Pakistan\",\n                    op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n                    ans: \"Islamabad\"\n                },\n\n                {\n                    q: \"What is the capital of Punjab\",\n                    op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n                    ans: \"Lahore\"\n                },\n\n                {\n                    q: \"What is the capital of Sindh\",\n                    op: [\"Lahore\", \"Karachi\", \"Pashawar\", \"Islamabad\"],\n                    ans: \"Karachi\"\n                }\n            ],\n            qno: 0, //Indexing Of Question No\n            show: \"\"\n        };\n    },\n    methods: {\n        checkAns: function() {\n            if (this.picked == this.show.ans) {\n                alert(\"correct\");\n                this.point += 2;\n                this.correctAns();\n                //          this.next(); //When the Answer is connect. point+2 and next question.\n            } else {\n                alert(\"wrong\");\n                this.point -= 2;\n                this.wrongAns();\n                //         this.next(); //When the Answer is connect. point-2 and next question.\n            }\n        },\n        //Next Function Basically, Brings up the next question.\n        next: function() {\n            if (this.qno < this.question.length - 1) {\n                this.qno++;\n                this.show = this.question[this.qno];\n                this.correct = false;\n                this.wrong = false;\n                this.normal = true;\n            } else {\n                console.log(this.qno);\n            }\n        },\n\n        correctAns: function() {\n            this.normal = false;\n            this.correct = true;\n            this.wrong = false;\n        },\n        wrongAns: function() {\n            var id = this.picked.split(\".\");\n            \n            for(i=0 ; i<4 ; i++)\n            {\n                if(this.question.ans == this.question.op[i])\n                    break;\n            }\n            console.log(i);\n            // var obj = document.getElementById(id[i]);\n\n            // obj.style.color = \"green\";\n\n\n            this.normal = false;\n            this.correct = false;\n            this.wrong = true;\n        }\n    },\n\n    //lifecycle\n    created() {\n        this.show = this.question[this.qno];\n    }\n};\n</script>\n\n\n<style scoped>\n.game {\n    width: 100%;\n    background-color: #e5e5e5;\n}\n\n.game .heading {\n    padding: 10px 9px 5px 5px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    background-color: #c4c4c4;\n    text-align: center;\n    margin-bottom: 80px;\n}\n\n.question {\n    padding: 4px 9px 4px 9px;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    text-align: left;\n}\n\n.normal {\n    background-color: #c4c4c4;\n}\n\n.correct {\n    background-color: #63e25d;\n}\n\n.wrong {\n    background-color: #c70039;\n}\n</style>"]}]}